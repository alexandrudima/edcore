{
    "files.insertFinalNewline": true,
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "files.associations": {
        "__locale": "cpp",
        "__string": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "__split_buffer": "cpp",
        "bitset": "cpp",
        "initializer_list": "cpp",
        "iterator": "cpp",
        "utility": "cpp",
        "vector": "cpp",
        "memory": "cpp",
        "array": "cpp",
        "*.tcc": "cpp",
        "system_error": "cpp",
        "functional": "cpp"
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "build/": true
    }
}
